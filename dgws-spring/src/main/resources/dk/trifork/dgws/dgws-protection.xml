<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dgwsProtectionAspect" class="com.trifork.dgws.aspect.DgwsProtectionAspect"/>

    <bean id="securityChecker" class="com.trifork.dgws.SecurityCheckerImpl"/>

    <bean id="securityHelper" class="com.trifork.dgws.util.SecurityHelperImpl"/>

    <bean id="dgwsRequestContext" class="com.trifork.dgws.DgwsRequestContextDefault"/>
</beans>